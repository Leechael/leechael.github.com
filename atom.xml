---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Leechael's Weblog</title>
 <link href="http://www.yan-yan.info/atom.xml" rel="self"/>
 <link href="http://www.yan-yan.info/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://www.yan-yan.info/</id>
 <author>
   <name>Leechael Yim</name>
   <email>yanleech@gmail.com</email>
 </author>

{% for post in site.posts %}
 {% if post.type == nil %}
 <entry>
   <title>{{ post.title | xml_escape }}</title>
   <link href="http://www.yan-yan.info{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://www.yan-yan.info{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endif %}
{% endfor %}
 
</feed>
